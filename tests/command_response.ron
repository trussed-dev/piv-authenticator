            ),
            GetData(
                input: "5C 03 5FC102",
                output: Data("53 3b 3019d4e739d821086c1084210d8360d8210842108421804210c3f3341000112233445566778899aabbccddeeff350839393939313233313e00fe00"),
            ),
        ]
    ),
